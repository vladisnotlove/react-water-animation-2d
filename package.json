    "url": "git+https://github.com/vladisnotlove/react-water-animation-2d.git"
  },
  "keywords": [
    "react",
    "animation",
    "animation-2d",
    "water-animation",
    "water-animation-2d",
    "canvas"
  ],
  "author": "Vladislav Nikolaev",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/vladisnotlove/react-water-animation-2d/issues"
  },
  "homepage": "https://github.com/vladisnotlove/react-water-animation-2d#readme",
  "dependencies": {
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "throttle-debounce": "^3.0.1",
    "use-resize-observer": "^8.0.0",
    "water-animation-2d": "^0.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.0",
    "@babel/core": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@types/node": "^17.0.16",
    "@types/throttle-debounce": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^8.2.0",
    "eslint-plugin-react": "^7.27.0",
    "typescript": "^4.4.4"
  }
