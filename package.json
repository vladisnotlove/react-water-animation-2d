{
  "name": "react-water-animation-2d",
  "version": "0.0.8",
  "description": "Wrapper of water-animation-2d for React",
  "main": "./dist/index.js",
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "dist/**/*.js.map",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "type-check": "tsc --noEmit",
